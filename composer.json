{
    "name": "cleaniquecoders/attendance",
    "description": "An Adaptive Attendance Solution",
    "keywords": ["package", "laravel", "attendance", "beacon", "BLE", "API", "Beacon"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Attendance\\": "src/"
        },
        "classmap": [
            "database/seeds"
        ],
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Attendance\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "cleaniquecoders/blueprint-macro": "^2.2",
        "cleaniquecoders/laravel-helper": "^1.1",
        "cleaniquecoders/laravel-observers": "^1.1",
        "illuminate/support": "^5.5|^5.6|^5.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\Attendance\\AttendanceServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}